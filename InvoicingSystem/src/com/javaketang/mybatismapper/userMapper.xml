<?xml version="1.0" encoding="UTF-8" ?> 
<!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.javaketang.mybatismapper.UserMapper">

	<select id="login" resultType="com.javaketang.entity.User" parameterType="com.javaketang.entity.User">
	
	select id,realName,userName from users where userName = #{userName} and password = #{password}
	
	</select>
	
</mapper>
